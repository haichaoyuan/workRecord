<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/global_blue_color"></solid>
            <stroke android:width="0.1dp" android:color="@color/global_gray_col"></stroke>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/global_white"></solid>
            <stroke android:width="0.1dp" android:color="@color/global_gray_col"></stroke>
        </shape>
    </item>
</selector>